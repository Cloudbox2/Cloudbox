---
backup:
  local:
    enable: yes
    destination: /mnt/local/Backups/Saltbox
  rclone:
    enable: yes
    destination: google:/Backups/Saltbox
  rsync:
    enable: no
    destination: rsync://somehost.com/Backups/Saltbox
    port: 22
  cron:
    cron_time: weekly
    cron_state: absent
  restore_service:
    user:
    pass:
  misc:
    snapshot: true
